    "devDependencies": {
        "@nestjs/cli": "^9.0.0",
        "@nestjs/schematics": "^9.0.0",
        "@nestjs/testing": "^9.0.0",
        "@types/cron": "^2.0.1",
        "@types/express": "^4.17.13",
        "@types/jest": "29.5.1",
        "@types/node": "18.16.12",
        "@types/supertest": "^2.0.11",
        "@typescript-eslint/eslint-plugin": "^7.4.0",
        "@typescript-eslint/parser": "^7.4.0",
